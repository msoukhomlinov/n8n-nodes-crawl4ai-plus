{
  "name": "n8n-nodes-crawl4ai-plus",
  "version": "2.0.2",
  "description": "n8n nodes for Crawl4AI v0.7.4+ web crawler and data extraction with enhanced features",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "crawl4ai",
    "web-crawler",
    "data-extraction",
    "scraping",
    "regex-extraction",
    "llm-extraction"
  ],
  "license": "MIT",
  "homepage": "https://github.com/msoukhomlinov/n8n-nodes-crawl4ai-plus",
  "author": [
    {
      "name": "Max Soukhomlinov",
      "email": "maxs@intellectit.com.au"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/msoukhomlinov/n8n-nodes-crawl4ai-plus.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"nodes/**/*.ts\" \"credentials/**/*.ts\"",
    "lint": "eslint --quiet --fix \"nodes/**/*.ts\" \"credentials/**/*.ts\"",
    "lintfix": "eslint . --fix",
    "prepublishOnly": "npm run build && npm run lint -c ./.eslintrc.prepublish.js"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Crawl4aiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Crawl4aiPlusBasicCrawler/Crawl4aiPlusBasicCrawler.node.js",
      "dist/nodes/Crawl4aiPlusContentExtractor/Crawl4aiPlusContentExtractor.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@types/request": "^2.48.12",
    "@types/request-promise-native": "^1.0.21",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "eslint-plugin-prettier": "^5.0.0",
    "gulp": "^4.0.2",
    "n8n-core": "^1.79.1",
    "n8n-workflow": "^1.82.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.10",
    "typescript": "~5.5.4"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
